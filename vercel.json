{
    "builds": [
        {
            "src": "shohor_bari/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "shohor_bari/wsgi.py"
        }
    ],
    "buildCommand": "pip install -r requirements.txt"
}